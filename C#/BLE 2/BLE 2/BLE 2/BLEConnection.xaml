<Page
    x:Class="BLE_2.BLEConnection"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BLE_2"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <RelativePanel>
            <Frame Margin="0,5,0,0">
                <AppBarButton Icon="Back" Click="AppBarButton_Click"/>
            </Frame>
            <Button x:Name="ConnectTactPlayButton" Content="ConnectTactPlay" HorizontalAlignment="Left" Margin="133,172,0,0" VerticalAlignment="Top" Click="ConnectTactPlay"/>
            <Button x:Name="Write1Button" Content="Write 1" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" Width="107" Margin="138,248,-179.667,-248" Click="Write1"/>
            <Button x:Name="write2Button" Content="Write 2" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" Width="107" Margin="138,320,-179.667,-320" Click="Write2"/>

        </RelativePanel>
    </Grid>
</Page>
